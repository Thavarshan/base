{
    "name": "thavarshan/base",
    "type": "project",
    "description": "A PHP composer package template.",
    "keywords": ["composer", "package"],
    "authors": [
        {
            "name": "Thavarshan Thayananthajothy",
            "email": "tjthavarshan@gmail.com"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2"
    },
    "require-dev": {
        "mockery/mockery": "^1.2.3",
        "phpunit/phpunit": "^8.3"
    },
    "autoload": {
        "psr-4": {
            "Base\\": "src/Base/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Base\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
